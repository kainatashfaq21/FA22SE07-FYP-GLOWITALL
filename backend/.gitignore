# Ignore files generated by the IDEs
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore build output and compiled files
/build/
/dist/
/out/
/bin/
/*.exe
/*.dll
/*.so
/*.dylib

# Ignore package managers and dependencies
/node_modules/
/vendor/
/.venv/

# Ignore operating system generated files
.DS_Store
Thumbs.db

# Ignore log files
*.log

# Ignore environment-specific files
.env
.env.local
.env.*.local

# Ignore backup files
*~
*.swp
*.bak

# Ignore sensitive or private information
*.key
*.pem
*.p12
*.password

# Ignore documentation and generated API files
/doc/
/docs/
/API/

# Ignore testing and debugging files
/coverage/
/.nyc_output/
/.idea/

# Ignore any custom files or directories specific to your project
/custom_directory/
custom_file.txt
